<div>
    <h1>List of Books using Handlebars:</h1>
    <div>
        {{#each result}}
            <div>
                <p>- {{this.title}}</p>
                <ul>
                    <li>$ {{this.price}}</li>
                    <li>{{this.description}}</li>
                    <li>Stock: {{this.stock}}</li>
                </ul>
            </div>
        {{/each}}
    </div>
</div>